<%= form_for :event , url: "/event/create_sections" do |form| %>
 

  <div class="field">
    <%= form.label :title %>
    <%= form.text_field :title, id: :event_title %>
  </div>

  <div class="field">
    <%= form.label :description %>
    <%= form.text_area :description, id: :event_description %>
  </div>

  <div class="field">
    <%= form.label :start_time %>
    <%= form.datetime_select :start_time, id: :event_start_time %>
  </div>

  <div class="field">
    <%= form.label :end_time %>
    <%= form.datetime_select :end_time, id: :event_end_time %>
  </div>

<div class="field">
    <%= form.label :nums_sections %>
    <%= form.text_field :numOfSections %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
