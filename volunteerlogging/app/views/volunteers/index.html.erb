<p id="notice"><%= notice %></p>

<h1>Volunteers</h1>

<table>
  <thead>
    <tr>
      <th>Volunteerid</th>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Email</th>
      <th>Encrypted password</th>
      <th>Datejoined</th>
      <th>Phonenum</th>
      <th>Voltype</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @volunteers.each do |volunteer| %>
      <tr>
        <td><%= volunteer.volunteerId %></td>
        <td><%= volunteer.firstName %></td>
        <td><%= volunteer.lastName %></td>
        <td><%= volunteer.email %></td>
        <td><%= volunteer.encrypted_password %></td>
        <td><%= volunteer.dateJoined %></td>
        <td><%= volunteer.phoneNum %></td>
        <td><%= volunteer.volType %></td>
        <td><%= link_to 'Show', volunteer %></td>
        <td><%= link_to 'Edit', edit_volunteer_path(volunteer) %></td>
        <td><%= link_to 'Destroy', volunteer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Volunteer', new_volunteer_path %>
